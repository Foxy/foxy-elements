{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "cancel": {
      "default": "Cancel",
      "type": "string"
    },
    "delete": {
      "default": "Delete",
      "type": "string"
    },
    "delete_prompt": {
      "default": "This resource will be permanently removed. Are you sure?",
      "type": "string"
    },
    "expires": {
      "default": "Expires",
      "type": "string"
    },
    "last_4_digits": {
      "default": "Last 4 digits",
      "type": "string"
    },
    "spinner": {
      "properties": {
        "loading_busy": {
          "$ref": "spinner.json#/properties/loading_busy"
        },
        "loading_empty": {
          "$ref": "spinner.json#/properties/loading_empty"
        },
        "loading_error": {
          "$ref": "spinner.json#/properties/loading_error"
        }
      },
      "required": ["loading_busy", "loading_empty", "loading_error"],
      "additionalProperties": false
    },
    "unknown": {
      "default": "Unknown",
      "type": "string"
    }
  },
  "required": [
    "cancel",
    "delete",
    "delete_prompt",
    "expires",
    "last_4_digits",
    "unknown",
    "spinner"
  ],
  "additionalProperties": false
}
