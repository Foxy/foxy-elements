{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "code": {
      "default": "Code",
      "type": "string"
    },
    "date": {
      "default": "{{value, date}}",
      "type": "string"
    },
    "fraud_risk": {
      "default": "Fraud risk: {{ score }}",
      "type": "string"
    },
    "price": {
      "default": "{{amount, price}}",
      "type": "string"
    },
    "spinner": {
      "properties": {
        "loading_busy": {
          "$ref": "spinner.json#/properties/loading_busy"
        },
        "loading_empty": {
          "$ref": "spinner.json#/properties/loading_empty"
        },
        "loading_error": {
          "$ref": "spinner.json#/properties/loading_error"
        }
      },
      "required": ["loading_busy", "loading_empty", "loading_error"],
      "additionalProperties": false
    },
    "unknown": {
      "default": "Unknown",
      "type": "string"
    }
  },
  "required": ["code", "date", "fraud_risk", "price", "unknown", "spinner"],
  "additionalProperties": false
}
