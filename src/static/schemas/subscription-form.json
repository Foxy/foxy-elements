  "required": [
    "cancel",
    "cancellation-form",
    "close",
    "confirm",
    "currency",
    "daily_plural",
    "daily",
    "day_plural",
    "day",
    "end_subscription",
    "frequency_label",
    "frequency",
    "item_plural",
    "month_plural",
    "month",
    "monthly_plural",
    "monthly",
    "next_transaction_date",
    "price_recurring",
    "price_twice_a_month",
    "spinner",
    "subscription_active",
    "subscription_cancelled",
    "subscription_failed",
    "subscription_inactive",
    "subscription_will_be_cancelled",
    "transaction_plural",
    "transactions-table",
    "twice_a_month",
    "undo_cancel",
    "undo_confirm",
    "undo_header",
    "undo_message",
    "week_plural",
    "week",
    "weekly_plural",
    "weekly",
    "year_plural",
    "year",
    "yearly_plural",
    "yearly"
  ],
  "additionalProperties": false
}
