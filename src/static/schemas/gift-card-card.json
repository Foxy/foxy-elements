{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "expires_after_value": {
      "default": "Expires {{value, relative}}",
      "type": "string"
    },
    "never_expires": {
      "default": "Never expires",
      "type": "string"
    },
    "spinner": {
      "properties": {
        "loading_busy": {
          "$ref": "spinner.json#/properties/loading_busy"
        },
        "loading_empty": {
          "$ref": "spinner.json#/properties/loading_empty"
        },
        "loading_error": {
          "$ref": "spinner.json#/properties/loading_error"
        }
      },
      "required": ["loading_busy", "loading_empty", "loading_error"],
      "additionalProperties": false
    }
  },
  "required": ["expires_after_value", "never_expires", "spinner"],
  "additionalProperties": false
}
