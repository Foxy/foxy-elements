{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "loading_busy": {
      "default": "Loading",
      "type": "string"
    },
    "loading_empty": {
      "default": "No data",
      "type": "string"
    },
    "loading_end": {
      "default": "No more data",
      "type": "string"
    },
    "loading_error": {
      "default": "Failed to load",
      "type": "string"
    },
    "loading_paused": {
      "default": "Paused",
      "type": "string"
    }
  },
  "required": ["loading_busy", "loading_empty", "loading_end", "loading_error", "loading_paused"],
  "additionalProperties": false
}
