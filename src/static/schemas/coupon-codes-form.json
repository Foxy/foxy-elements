{
  "$schema": "http://json-schema.org/draft-07/schema",
  "additionalProperties": false,
  "properties": {
    "code": { "type": "string", "default": "Code" },
    "code_import_hint": {
      "type": "string",
      "default": "Paste multiple space-separated codes in the field above to add all of them at once. Importing duplicate codes clears their usage stats."
    },
    "code_plural": { "type": "string", "default": "Codes" },
    "duplicate": { "type": "string", "default": "Duplicate" },
    "hidden_codes_explainer": {
      "type": "string",
      "default": "We’ve hidden some of your codes because there were too many of them. Don’t worry – we’ll import them anyway."
    },
    "hidden_codes_header": { "type": "string", "default": "{{count}} code is hidden" },
    "hidden_codes_header_plural": { "type": "string", "default": "{{count}} codes are hidden" },
    "import": { "type": "string", "default": "Import" },
    "import_codes_done": {
      "type": "string",
      "default": "All done! We've imported your codes into the system."
    },
    "loading_busy": { "type": "string", "default": "Loading" },
    "loading_error": { "type": "string", "default": "Unknown error" },
    "type_here": { "type": "string", "default": "Type here..." },
    "unique": { "type": "string", "default": "Unique" },
    "v8n_required": { "type": "string", "default": "Required" },
    "spinner": {
      "additionalProperties": false,
      "properties": {
        "refresh": { "type": "string", "default": "Refresh" },
        "loading_busy": { "type": "string", "default": "Loading" },
        "loading_error": { "type": "string", "default": "Unknown error" }
      },
      "required": ["refresh", "loading_busy", "loading_error"]
    }
  },
  "required": [
    "code",
    "code_import_hint",
    "code_plural",
    "duplicate",
    "hidden_codes_explainer",
    "hidden_codes_header",
    "hidden_codes_header_plural",
    "import",
    "import_codes_done",
    "loading_busy",
    "loading_error",
    "type_here",
    "unique",
    "v8n_required",
    "spinner"
  ]
}
