{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "status_test": { "type": "string", "default": "Test" },
    "status_live": { "type": "string", "default": "Live" },
    "spinner": {
      "properties": {
        "loading_busy": {
          "$ref": "spinner.json#/properties/loading_busy"
        },
        "loading_empty": {
          "$ref": "spinner.json#/properties/loading_empty"
        },
        "loading_error": {
          "$ref": "spinner.json#/properties/loading_error"
        }
      },
      "required": ["loading_busy", "loading_empty", "loading_error"],
      "additionalProperties": false
    }
  },
  "required": ["status_test", "status_live", "spinner"],
  "additionalProperties": false
}
