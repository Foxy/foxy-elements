{
  "$schema": "http://json-schema.org/draft-07/schema",
  "additionalProperties": false,
  "properties": {
    "cancel": { "type": "string", "default": "Cancel" },
    "close": { "type": "string", "default": "Close" },
    "confirm": { "type": "string", "default": "Confirm" },
    "date": { "type": "string", "default": "{{value, date}}" },
    "save": { "type": "string", "default": "Save" },
    "time": { "type": "string", "default": "{{value, time}}" },
    "undo_cancel": { "type": "string", "default": "Review" },
    "undo_confirm": { "type": "string", "default": "Discard" },
    "undo_header": { "type": "string", "default": "Unsaved changes" },
    "undo_message": {
      "type": "string",
      "default": "Looks like you didn't save your changes! What would you like to do with them?"
    },
    "update": { "type": "string", "default": "Update" },
    "spinner": {
      "additionalProperties": false,
      "properties": {
        "refresh": { "type": "string", "default": "Refresh" },
        "loading_busy": { "type": "string", "default": "Loading" },
        "loading_empty": { "type": "string", "default": "No data" },
        "loading_error": { "type": "string", "default": "Unknown error" }
      },
      "required": ["refresh", "loading_busy", "loading_empty", "loading_error"]
    },
    "user-form": {
      "additionalProperties": false,
      "properties": {
        "backend_developer": { "type": "string", "default": "Backend developer" },
        "backend_developer_explainer": {
          "type": "string",
          "default": "Manages server-side code in PHP, Python, Ruby, etc."
        },
        "cancel": { "type": "string", "default": "Cancel" },
        "create": { "type": "string", "default": "Create" },
        "delete": { "type": "string", "default": "Delete" },
        "delete_prompt": {
          "type": "string",
          "default": "This resource will be permanently removed. Are you sure?"
        },
        "designer": { "type": "string", "default": "Designer" },
        "designer_explainer": {
          "type": "string",
          "default": "Manages appearance, art and user interface."
        },
        "email": { "type": "string", "default": "Email" },
        "first_name": { "type": "string", "default": "First name" },
        "frontend_developer": { "type": "string", "default": "Frontend developer" },
        "frontend_developer_explainer": {
          "type": "string",
          "default": "Manages client-side code in JS, CSS, HTML, etc."
        },
        "hidden": { "type": "string", "default": "Hidden" },
        "last_name": { "type": "string", "default": "Last name" },
        "merchant": { "type": "string", "default": "Merchant" },
        "merchant_explainer": {
          "type": "string",
          "default": "Manages items, finances and this store in general."
        },
        "phone": { "type": "string", "default": "Phone" },
        "v8n_invalid_email": { "type": "string", "default": "Invalid email" },
        "v8n_required": { "type": "string", "default": "Required" },
        "v8n_too_long": { "type": "string", "default": "Too long" },
        "spinner": {
          "additionalProperties": false,
          "properties": {
            "refresh": { "type": "string", "default": "Refresh" },
            "loading_busy": { "type": "string", "default": "Loading" },
            "loading_error": { "type": "string", "default": "Unknown error" }
          },
          "required": ["refresh", "loading_busy", "loading_error"]
        }
      },
      "required": [
        "backend_developer",
        "backend_developer_explainer",
        "cancel",
        "create",
        "delete",
        "delete_prompt",
        "designer",
        "designer_explainer",
        "email",
        "first_name",
        "frontend_developer",
        "frontend_developer_explainer",
        "hidden",
        "last_name",
        "merchant",
        "merchant_explainer",
        "phone",
        "v8n_invalid_email",
        "v8n_required",
        "v8n_too_long",
        "spinner"
      ]
    }
  },
  "required": [
    "cancel",
    "close",
    "confirm",
    "date",
    "save",
    "time",
    "undo_cancel",
    "undo_confirm",
    "undo_header",
    "undo_message",
    "update",
    "spinner",
    "user-form"
  ]
}
