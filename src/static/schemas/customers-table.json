{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "date": {
      "default": "{{value, date}}",
      "type": "string"
    },
    "date_created": {
      "default": "Created on",
      "type": "string"
    },
    "spinner": {
      "properties": {
        "loading_busy": {
          "$ref": "spinner.json#/properties/loading_busy"
        },
        "loading_empty": {
          "$ref": "spinner.json#/properties/loading_empty"
        },
        "loading_error": {
          "$ref": "spinner.json#/properties/loading_error"
        }
      },
      "required": ["loading_busy", "loading_error", "loading_empty"],
      "additionalProperties": false
    }
  },
  "required": ["date", "date_created", "spinner"],
  "additionalProperties": false
}
