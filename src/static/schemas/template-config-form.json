    "foxycomplete_disabled_explainer": {
      "type": "string",
      "default": "Let customers pick an option from a list"
    },
    "foxycomplete_search": { "type": "string", "default": "Search" },
    "foxycomplete_search_explainer": {
      "type": "string",
      "default": "Display a «search as you type» field"
    },
