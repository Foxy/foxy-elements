{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "percent": {
      "default": "{{fraction, percent}}",
      "type": "string"
    },
    "spinner": {
      "properties": {
        "loading_busy": {
          "$ref": "spinner.json#/properties/loading_busy"
        },
        "loading_empty": {
          "$ref": "spinner.json#/properties/loading_empty"
        },
        "loading_error": {
          "$ref": "spinner.json#/properties/loading_error"
        }
      },
      "required": ["loading_busy", "loading_empty", "loading_error"],
      "additionalProperties": false
    },
    "tax_global": {
      "default": "Global tax",
      "type": "string"
    },
    "tax_rate_provider_default": {
      "default": "Default (Thomson Reuters; others)",
      "type": "string"
    },
    "tax_union": {
      "default": "European Union tax",
      "type": "string"
    }
  },
  "required": ["percent", "tax_global", "tax_rate_provider_default", "tax_union", "spinner"],
  "additionalProperties": false
}
