{
  "$schema": "http://json-schema.org/draft-07/schema",
  "additionalProperties": false,
  "properties": {
    "cancel": { "type": "string", "default": "Cancel" },
    "confirm": { "type": "string", "default": "Confirm" },
    "balance": { "type": "string", "default": "Balance" },
    "current_balance": { "type": "string", "default": "Current balance" },
    "generate": { "type": "string", "default": "Generate" },
    "generate_codes_done": {
      "type": "string",
      "default": "All done! We've added new codes for you."
    },
    "leave_empty_for_random_codes": {
      "type": "string",
      "default": "Leave empty to generate completely random codes."
    },
    "length": { "type": "string", "default": "Length" },
    "number_of_codes": { "type": "string", "default": "Number of codes" },
    "prefix": { "type": "string", "default": "Prefix" },
    "template": { "type": "string", "default": "Template" },
    "v8n_negative": { "type": "string", "default": "Negative values are not allowed" },
    "v8n_required": { "type": "string", "default": "Required" },
    "v8n_prefix_has_spaces": {
      "type": "string",
      "default": "Spaces are not allowed in code prefix"
    },
    "spinner": {
      "additionalProperties": false,
      "properties": {
        "refresh": { "type": "string", "default": "Refresh" },
        "loading_busy": { "type": "string", "default": "Loading" },
        "loading_error": { "type": "string", "default": "Unknown error" }
      },
      "required": ["refresh", "loading_busy", "loading_error"]
    }
  },
  "required": [
    "cancel",
    "confirm",
    "balance",
    "current_balance",
    "generate",
    "generate_codes_done",
    "leave_empty_for_random_codes",
    "length",
    "number_of_codes",
    "prefix",
    "template",
    "v8n_negative",
    "v8n_required",
    "v8n_prefix_has_spaces",
    "spinner"
  ]
}
