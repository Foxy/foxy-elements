{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "end_date": {
      "default": "End date",
      "type": "string"
    },
    "end_subscription": {
      "default": "End subscription",
      "type": "string"
    },
    "end_subscription_explainer": {
      "default": "Your subscription will remain active until the selected end date. You might have to resubscribe if you decide to resume this subscription later.",
      "type": "string"
    },
    "spinner": {
      "properties": {
        "loading_busy": {
          "$ref": "spinner.json#/properties/loading_busy"
        },
        "loading_error": {
          "$ref": "spinner.json#/properties/loading_error"
        }
      },
      "required": ["loading_busy", "loading_error"],
      "additionalProperties": false
    },
    "v8n_required": {
      "default": "Required",
      "type": "string"
    }
  },
  "required": [
    "end_date",
    "end_subscription",
    "end_subscription_explainer",
    "v8n_required",
    "spinner"
  ],
  "additionalProperties": false
}
