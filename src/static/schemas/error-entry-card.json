{
  "$schema": "http://json-schema.org/draft-07/schema",
  "additionalProperties": false,
  "properties": {
    "client": { "type": "string", "default": "Client" },
    "date": { "type": "string", "default": "{{value, date}}" },
    "error": { "type": "string", "default": "Error" },
    "request": { "type": "string", "default": "Request" },
    "time": { "type": "string", "default": "{{value, time}}" }
  },
  "required": ["client", "date", "error", "request", "time"]
}
